<!-- 基本資料 -->
<div class="basedata" style="width:1000px;">
    <h1 class="x110000" style="text-align: center;font-family: Arial,標楷體;font-weight: bold;">
        <%= @yfcase.case_number %> YF評量表 </br>
        <small>(所屬公司：<%= @yfcase.company %>)</small>
    </h1>
    <table style="border:3px solid black;">
        <tr>
            <th class="xa1100 font-th">案號</th>
            <th class="xa1200 font-td"><%= @yfcase.case_number %></th>
            <th class="xa1300 font-th">債務人</th>
            <th class="xa1400 font-td">
            	<% if @yfcase.personnals.where(is_debtor: true).count > 1 %>
                    <%= @yfcase.personnals.where(is_debtor: true).first.name %> ...共<%= @yfcase.personnals.where(is_debtor: true).count %>名<br>
                    <%= (@yfcase.co_owner == true ) ? '土地/建物共有人一致' : ' ' %>
                <% elsif @yfcase.personnals.where(is_debtor: true).count == 1 %>
                    <%= @yfcase.personnals.where(is_debtor: true).first.name %><br>
                    <%= (@yfcase.co_owner == true ) ? '土地/建物共有人一致' : ' ' %>
                <% else %>
                    <%= ' ' %>
                <% end %>
            </th>
            <th class="xa1500 font-th">債權人</th>
            <th class="xa1600 font-td">
                <% if @yfcase.personnals.where(is_creditor: true).count > 1 %>
                    <%= @yfcase.personnals.where(is_creditor: true).first.name %> ...共<%= @yfcase.personnals.where(is_creditor: true).count %>名
                <% elsif @yfcase.personnals.where(is_creditor: true).count == 1 %>
                    <%= @yfcase.personnals.where(is_creditor: true).first.name %>
                <% else %>
                    <%= ' ' %>
                <% end %>
            </th>
        </tr>
        <tr>
            <th class="xa2100 font-th">住址</th>
            <th class="xa2200 font-td" colspan="5">
                <%= @yfcase.township.try(:zip_code) %> <%= @yfcase.country.try(:name) %><%= \
                @yfcase.township.try(:name) %><%= @yfcase.other_address %>
            </th>
        </tr>
    </table>
</div>
<div style="padding:3px;"></div>

<!-- 土地 -->
<div class="land" style="width:1000px;">
    <table>
        <tr>
            <th class="xb1100 font-th">地號</th>
            <th class="xb1200 font-th">地號謄本</th>
            <th class="xb1300 font-th">地坪(面積)<br>平方公尺(坪)</th>
            <th class="xb1400 font-th">持分</th>
            <th class="xb1500 font-th">地坪(持分)<br>平方公尺(坪)</th>
            <th class="xb1600 font-th"></th>
            <th class="xb1700 font-th">地坪(面積)<br>平方公尺(坪)</th>
            <th class="xb1800 font-th"></th>
            <th class="xb1900 font-th">建坪(面積)<br>平方公尺(坪)</th>
        </tr>
        <tr>
            <th class="xb2100 font-td">
                <% @yfcase.lands.each do |land| %>
                    <% if @yfcase.big_section.blank? %>
                        <%= land.land_number %><br>
                    <% else %>
                        <%= @yfcase.big_section %><%= land.land_number %><br>
                    <% end %>
                <% end %>
            </th>
            <th class="xb2200 font-td">
                <% @yfcase.lands.each do |land| %>
                    <%= land.land_url == "" ? ' ' : (link_to "連結", land.land_url, target: "_blank") %><br>
                <% end %>
            </th>
            <th class="xb2300 font-td">
                <% @yfcase.lands.each do |land| %>
                    <%= area(land.land_area,1,1,1).round(2) %>(<%= area(land.land_area,1,1,0.3025).round(2) %>)<br>
                <% end %>
            </th>
            <th class="xb2400 font-td">
                <% @yfcase.lands.each do |land| %>
                    <%= land.land_holding_point_personal %>/<%= land.land_holding_point_all %><br>
                <% end %>
            </th>
            <th class="xb2500 font-td">
                <% @yfcase.lands.each do |land| %>
                    <%= area(land.land_area,land.land_holding_point_personal,land.land_holding_point_all,1).round(2) %>(<%= area(land.land_area,land.land_holding_point_personal,land.land_holding_point_all,0.3025).round(2) %>)<br>
                <% end %>
            </th>
            <th class="xb2600 font-td"></th>
            <th class="xb2700 font-td">
                <%= @yfcase.lands.count == 0 ? ' ' : (number_to_currency(@landtotalarea, unit: "", precision: 2)+"("+number_to_currency(@landtotalarea * 0.3025, unit: "", precision: 2)+")") %><br>
            </th>
            <th class="xb2800 font-td"></th>
            <th class="xb2900 font-td">
                <% if @yfcase.builds.count == 0 %>
                    ' ' 
                <% elsif @yfcase.builds.where(build_type_use: "0公設").count > 0 %>
                    <%= number_to_currency(@withBuildTotalArea+@withoutBuildTotalArea, unit: "", precision: 2)+"("+number_to_currency((@withBuildTotalArea+@withoutBuildTotalArea) * 0.3025, unit: "", precision: 2)+")" %><br>
                <% else %>
                    <%= number_to_currency(@withoutBuildTotalArea, unit: "", precision: 2)+"("+number_to_currency(@withoutBuildTotalArea * 0.3025, unit: "", precision: 2)+")" %><br>
                <% end %>
            </th>
        </tr>
    </table>
</div>
<div style="padding:3px;"></div> 


<!-- 建物 -->
<div class="build" style="width:1000px;">
    <table>
        <tr>
            <th class="xc1100 font-th">建號</th>
            <th class="xc1200 font-th">建號謄本</th>
            <th class="xc1300 font-th">建坪(面積)<br>平方公尺(坪)</th>
            <th class="xc1400 font-th">持分</th>  
            <th class="xc1500 font-th">建坪(持分)<br>平方公尺(坪)</th>
            <th class="xc1600 font-th">建物型/使用</th>
            <th class="xc1700 font-th">使用分區</th>
        </tr>
        <tr>
            <th class="xc2100 font-td">
        <% @yfcase.builds.each do |build| %>
            <% if @yfcase.big_section.blank? %>
                <%= build.build_number %><br>
            <% else %>
                <%= @yfcase.big_section %><%= build.build_number %><br>
            <% end %>
        <% end %>
            </th>
            <th class="xc2200 font-td">
        <% @yfcase.builds.each do |build| %>
                <% if build.build_url == "" %>
                <%= link_to " " %> <br>
                <% else%>
                <%= link_to "連結", build.build_url, target: "_blank" %><br>
                <% end %>
        <% end %>
            </th>
            <th class="xc2300 font-td">
        <% @yfcase.builds.each do |build| %>
                <%= area(build.build_area,1,1,1).round(2) %> (
                <%= area(build.build_area,1,1,0.3025).round(2) %>)<br>
        <% end %>
            </th>
            <th class="xc2400 font-td">
        <% @yfcase.builds.each do |build| %>
            <% if build.build_type_use == "0公設" %> 
                <!--找到第一筆非公設(notPU)的個人持分(holding_point_personal)-->
                <% @notPU_holding_point_personal=@yfcase.builds.where.not(build_type_use: "0公設").first.build_holding_point_personal %>
                <!--找到第一筆非公設(notPU)的個人持分(build_holding_point_all)-->
                <% @notPU_build_holding_point_all=@yfcase.builds.where.not(build_type_use: "0公設").first.build_holding_point_all %>
                <%= @notPU_holding_point_personal*build.build_holding_point_personal %> / <%= @notPU_build_holding_point_all*build.build_holding_point_all %><br>
            <% else %>
                <%= build.build_holding_point_personal %>/
                <%= build.build_holding_point_all %><br>
            <% end %>
        <% end %>
            </th>
            <th class="xc2500 font-td">
        <% @yfcase.builds.each do |build| %>
            <% if build.build_type_use == "0公設" %> 
                <!--找到第一筆非公設(notPU)的個人持分(holding_point_personal)-->
                <% @notPU_holding_point_personal=@yfcase.builds.where.not(build_type_use: "0公設").first.build_holding_point_personal %>
                <!--找到第一筆非公設(notPU)的個人持分(build_holding_point_all)-->
                <% @notPU_build_holding_point_all=@yfcase.builds.where.not(build_type_use: "0公設").first.build_holding_point_all %>
                <%= area(build.build_area,@notPU_holding_point_personal*build.build_holding_point_personal,@notPU_build_holding_point_all*build.build_holding_point_all,1).round(2) %> (
                <%= area(build.build_area,@notPU_holding_point_personal*build.build_holding_point_personal,@notPU_build_holding_point_all*build.build_holding_point_all,0.3025).round(2) %>)<br>
            <% else %>
                <%= area(build.build_area,build.build_holding_point_personal,build.build_holding_point_all,1).round(2) %> (
                <%= area(build.build_area,build.build_holding_point_personal,build.build_holding_point_all,0.3025).round(2) %>)<br>
            <% end %>
        <% end %>
            </th>
            <th class="xc2600 font-td">
        <% @yfcase.builds.each do |build| %>
                <%= build.build_type_use %><br>
        <% end %>
            </th>
            <th class="xc2700 font-td">
        <% @yfcase.builds.each do |build| %>
                <%= build.use_partition %><br>
        <% end %>
            </th>
        </tr>
    </table>
</div>
<div style="padding:3px;"></div>

<!-- 拍賣結果 -->
<div class="auction" style="width:1000px;">
    <table>
        <tr>
            <th class="xd1100 font-th-12">拍賣日</th>
            <th class="xd1200 font-th-12" colspan="4">拍別-進場判定<br><div style="padding:0px;font-size:10pt;color:red;">(可進場CP值)</div></th>
            <th class="xd1600 font-th-12">底價(NT)<br><div style="padding:0px;font-size:10pt;color:red;">各拍建議底價</div></th>
            <th class="xd1700 font-th-12">坪價(NT)</th>
            <th class="xd1800 font-th-12">時價(NT)</th>
            <th class="xd1900 font-th-12">CP</th>
            <th class="xd1a00 font-th-12">點閱</th>
            <th class="xd1b00 font-th-12">監控</th>
            <th class="xd1c00 font-th-12">建議加價</th>
            <th class="xd1d00 font-th-12">保証金</th>

        </tr>
        <!-- 第一拍 -->
        <tr>
            <!-- 拍別：第一拍 -->
            <th class="xd2100 font-td-12" style="color:blue;">
                <%= @yfcase.floor_price_1 == 0 ? ' ' : @yfcase.auction_day_1 %>
            </th>
            <th class="xd2200 font-td-12">第一拍<br><div style="padding:0px;font-size:10pt;color:red;">(CP>0.92)</div></th>
            <th class="xd2300 font-td-10-bk" colspan="3">
                <% @floorPricePlus1=suggestedincreaseJudgment(@yfcase.floor_price_1,@yfcase.click_1,@yfcase.monitor_1) %>
                <% @cp1=@marketprice / @pingprice1 %>
                <% @cpPlusJudgment1=@cp1 / @floorPricePlus1 %>
                <% if @yfcase.floor_price_1 == 0 %>
                    <div>底價未輸入</div>
                <% elsif @cpPlusJudgment1 > 0.92 %>
                    <div style="padding:0px;font-size:10pt;color:green;">加價CP(<%= (@cpPlusJudgment1).round(2) %>)<br>建議進場</div>
                <% else %>
                    <div style="padding:0px;font-size:10pt;color:red;">加價CP(<%= (@cpPlusJudgment1).round(2) %>)<br>不可進場</div>
                <% end %>
            </th>
            <!-- 第一拍：底價 -->
            <th class="xd2600 font-td-12" style="color:blue;">
                <%= @yfcase.floor_price_1 == 0 ? ' ' : currency_helper(@yfcase.floor_price_1,0) %>
            </th>
            <!-- 第一拍：坪價 -->
            <th class="xd2700 font-td-12" style="color:blue;">
                <%= (@yfcase.floor_price_1 == 0 || @yfcase.builds.count == 0) ? ' ' : currency_helper(@pingprice1,0) %>
            </th>
            <!-- 第一拍：時價 -->
            <th class="xd2800 font-td-12" style="color:blue;">
                <%= (@yfcase.floor_price_1 == 0 || @yfcase.objectbuilds.count == 0) ? ' ' : currency_helper(@marketprice,0) %> 
            </th>
            <!-- 第一拍：CP值 -->
            <th class="xd2900 font-td-12" style="color:blue;">
                <%= (@yfcase.floor_price_1 == 0 || @yfcase.builds.count == 0 || @yfcase.objectbuilds.count == 0) ? ' ' : (@marketprice / @pingprice1).round(2) %>
            </th>
            <!-- 第一拍：點閱 -->
            <th class="xd2a00 font-td-12" style="color:blue;">
                <%= @yfcase.floor_price_1 == 0 ? ' ' : @yfcase.click_1 %>
            </th>
            <!-- 第一拍：監控 -->
            <th class="xd2b00 font-td-12" style="color:blue;">
                <%= @yfcase.floor_price_1 == 0 ? ' ' : @yfcase.monitor_1 %>
            </th>
            <!-- 第一拍：建議加價 -->
            <th class="xd2c00 font-td-10">
                <%= @yfcase.floor_price_1 == 0 ? ' ' : suggestedincrease(@yfcase.floor_price_1,@yfcase.click_1,@yfcase.monitor_1) %>
            </th>
            <!-- 第一拍：保証金 -->
            <th class="xd2d00 font-td-12" style="color:blue;">
                <%= @yfcase.floor_price_1 == 0 ? ' ' : currency_helper(@yfcase.margin_1,0) %>
            </th>

        </tr>
        <!-- 第二拍 -->
        <tr>
            <!-- 拍別：第二拍 -->
            <th class="xd3100 font-td-12" style="color:blue;">
                <%= @yfcase.floor_price_2 == 0 ? ' ' : @yfcase.auction_day_2 %>
            </th>
            <th class="xd3200 font-td-12">第二拍<br><div style="padding:0px;font-size:10pt;color:red;">(CP>1.15)</div></th>
            <th class="xd3300 font-td-10-bk" colspan="3">
                <% @floorPricePlus2=suggestedincreaseJudgment(@yfcase.floor_price_2,@yfcase.click_2,@yfcase.monitor_2) %>
                <% @cp2=@marketprice / @pingprice2 %>
                <% @cpPlusJudgment2=@cp2 / @floorPricePlus2 %>
                <% if @yfcase.floor_price_2 == 0 %>
